{
  "hash": "082aad31c87372b15c27df8342c958a131ca100efbbe9891f2f904b1778c1905",
  "type": "plugin",
  "about": {
    "name": {
      "default": "Audio Presets",
      "tr": "Ses Ön Ayarları"
    },
    "description": {
      "default": "Allows you to use custom audio presets.",
      "tr": "Özel ses ön ayarları kullanmanıza olanak sağlar."
    },
    "authors": [
      {
        "name": "thearmagan",
        "id": "707309693449535599"
      }
    ],
    "version": "0.0.3",
    "license": "MIT",
    "readme": "./readme.md",
    "previews": []
  },
  "api": {
    "modules": {
      "custom": [
        {
          "name": "WebAudio",
          "finder": {
            "filter": {
              "export": false,
              "in": "prototypes",
              "by": [
                [
                  "_ensureAudio"
                ]
              ]
            },
            "path": {
              "before": [
                "exports.Z",
                "exports.ZP",
                "exports.default",
                "exports"
              ],
              "after": "WebAudio"
            },
            "map": {
              "WebAudio": [
                "prototype._ensureAudio"
              ]
            }
          }
        }
      ]
    }
  },
  "mode": "development",
  "config": [
    {
      "type": "Column",
      "children": [
        {
          "type": "Heading",
          "value": "Preset"
        },
        {
          "type": "Paragraph",
          "value": "Your audio preset configuration.. Supports swapping sounds and also custom sounds from discord cdn(uploads) too. Format is original_name=new_name or original_name=https://discordcdnlink"
        },
        {
          "type": "Textarea",
          "default": "message1=message2\ncall_ringing=call_ringing_beat",
          "id": "preset",
          "placeholder": "message1=message2\ncall_ringing=https://discordcdnlink"
        }
      ]
    },
    {
      "type": "Row",
      "children": [
        {
          "type": "Column",
          "children": [
            {
              "type": "Heading",
              "value": "Log Audio Names"
            },
            {
              "type": "Paragraph",
              "value": "Helpful when trying to identify which sound is which."
            }
          ]
        },
        {
          "type": "Check",
          "default": false,
          "id": "logAudioNames"
        }
      ]
    }
  ],
  "readme": true
}